-if @supplier
  #help
    %h2 Help
    %ul
      %li Here you can change the dates of a delivery
%h1 Edit a Delivery
=form_for @delivery do |d|
  %fieldset
    %ul
      %li
        =d.label :date
        =d.date_select :date
      %li
        =d.label :last_order
        =d.datetime_select "last_order"
    %ul
      %li
        %label
        =submit_tag 'Update'
